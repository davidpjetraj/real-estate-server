  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "push": "npx prisma db push",
    "gen": "npx prisma generate",
    "prisma": "npx prisma studio",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/admin/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/client/test/jest-e2e.json",
    "deploy:prepare": "yarn install && yarn gen && yarn build",
    "deploy:start": "yarn start:prod",
    "postinstall": "yarn gen"
  },
